    "noImplicitAny": true,
    "module": "es2022",
    "target": "es5",
    "baseUrl": "src",
    "jsx": "react-jsx",
    "allowJs": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "strict": true
  }
}
